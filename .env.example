# NEONSYNC Environment Variables
# Copy this file to .env and fill in your actual values

# Somnia Testnet RPC URL
SOMNIA_TESTNET_RPC=https://dream-rpc.somnia.network

# Your wallet private key (DO NOT COMMIT THIS - Keep in .env only)
PRIVATE_KEY=your_private_key_here

# Somnia API Key for contract verification (optional)
SOMNIA_API_KEY=your_api_key_here

# WalletConnect Project ID (Get from https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Deployed Contract Address (fill after deployment)
VITE_NEON_ARENA_CONTRACT_ADDRESS=

# Somnia Data Streams Configuration
VITE_SDS_ENDPOINT=wss://streams.somnia.network
VITE_SDS_API_KEY=your_sds_api_key
